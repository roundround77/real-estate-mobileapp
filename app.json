{
  "expo": {
    "name": "RealEstate",
    "slug": "RealEstate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAuZHMMzsc6VmgLreSCXMKAagpAXjuuiGA"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is needed to show your current position on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAuZHMMzsc6VmgLreSCXMKAagpAXjuuiGA"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
